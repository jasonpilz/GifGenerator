<% if current_admin? %>
  <%= link_to "Logout", logout_path, method: :delete %>
  <%= link_to "Create Gif", new_admin_gif_path %>
  <%= link_to "Create Category", new_admin_category_path %>
<% elsif current_user %>
  <%= link_to "Logout", logout_path, method: :delete %>
<% else %>
  <%= link_to "Login", login_path %>
  <%= link_to "Create Account", new_user_path %>
<% end %>
